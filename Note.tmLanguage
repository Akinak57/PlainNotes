        [ \t]*          # Optional whitespace
        (&lt;?)(\S+?)(&gt;?)      # The url
        [ \t]*          # Optional whitespace
        (?:
            ((\().+?(\)))   # Match title in quotes…
          | ((['"]).+?(['"]))   # or in parens.
        )?            # Title is optional
        [ \t]*            # Optional whitespace
